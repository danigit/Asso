<?xml version='1.0' encoding='utf-8'?>
<widget id="com.resolute.Asso" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Asso Antincendio</name>
    <description>
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
    </author>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.InAppBrowser" />
    </feature>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-navigation href="http://www.assoantincendio.com/areaclienti/index.php" />
    <allow-navigation href="http://www.assoantincendio.com/areaclienti/*" />
    <allow-navigation href="http://www.assoantincendio.com/areaclienti/*/*" />
    <!--<allow-navigation href="http://www.assoantincendio.com/areaclienti/*/*/*" />-->
    <!--<allow-navigation href="http://danielfotografo.altervista.org/index.php" />-->
    <!--<allow-navigation href="http://danielfotografo.altervista.org/*" />-->
    <!--<allow-navigation href="http://danielfotografo.altervista.org/*/*" />-->
    <!--<allow-navigation href="http://danielfotografo.altervista.org/*/*/*" />-->
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-inappbrowser" spec="^3.0.0" />
    <engine name="browser" spec="^5.0.4" />
    <engine name="android" spec="^7.1.1" />
</widget>
